{
  "name": "@transferwise/neptune-docs",
  "version": "10.0.31",
  "description": "TransferWise Design System",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:compile-css": "gulp watchLess --src='static/assets' --dest='static/assets'",
    "dev:next": "node ./docgen.js && next dev",
    "docs": "yarn dev",
    "build": "npm-run-all build:*",
    "build:compile-css": "gulp compileLess --src='static/assets' --dest='static/assets'",
    "build:docs-gen": "node ./docgen.js && node ./utils/bundleSize/getBundleSize.js",
    "build:next": "next build",
    "docs:deploy": "npm-run-all deploy:*",
    "deploy:gh-pages": "deploy-to-github-pages -d docs-build -m main -c 15 -b deploy-cleanup",
    "lint": "jest --maxWorkers=4 --config ../../node_modules/@transferwise/test-config/test/jest.lint.js",
    "lint:fix": "eslint components/ pages/ utils/ --fix"
  },
  "repository": {
    "type": "git",
    "fullname": "transferwise/neptune-web",
    "url": "git+https://github.com/transferwise/neptune-web.git"
  },
  "keywords": [],
  "author": {
    "name": "TransferWise",
    "url": "https://transferwise.com/"
  },
  "bugs": {
    "url": "https://github.com/transferwise/neptune-web/issues"
  },
  "homepage": "https://transferwise.github.io/neptune-web",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@mapbox/rehype-prism": "^0.5.0",
    "@mdx-js/loader": "^1.5.8",
    "@next/mdx": "^9.0.5",
    "@transferwise/components": "^34.3.3",
    "@transferwise/dynamic-flows": "^11.4.2",
    "@transferwise/icons": "^2.6.0",
    "@transferwise/neptune-css": "^8.1.1",
    "@transferwise/test-config": "^3.0.2",
    "@zeit/next-css": "^1.0.1",
    "bootstrap": "github:transferwise/bootstrap#semver:^5.20.0",
    "classnames": "^2.2.6",
    "currency-flags": "^2.1.1",
    "date-fns": "^2.17.0",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "lint-staged": "^9.4.2",
    "next": "^9.5.5",
    "next-images": "^1.1.2",
    "next-transpile-modules": "^2.3.1",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.0",
    "react-docgen": "^5.3.1",
    "react-docgen-external-proptypes-handler": "^2.0.0",
    "react-live": "^2.2.2",
    "react-required-if": "^1.0.3",
    "rehype-slug": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@transferwise/less-config": "^3.0.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "deploy-to-github-pages": "1.0.0-beta.7"
  },
  "peerDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "resolutions": {
    "lint-staged/**/kind-of": "^6.0.3",
    "next/**/kind-of": "^6.0.3"
  }
}
