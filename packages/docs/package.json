{
  "name": "@transferwise/neptune-docs",
  "version": "4.2.15",
  "description": "TransferWise Design System",
  "main": "index.js",
  "private": true,
  "scripts": {
    "docs": "node ./docgen.js && next dev",
    "build": "node ./docgen.js && node ./utils/bundleSize/getBundleSize.js && next build && yarn build:compile-css",
    "build:compile-css": "gulp compileLess --src='static/assets' --dest='static/assets'",
    "docs:build": "next export -o docs-build && touch docs-build/.nojekyll",
    "docs:deploy": "gh-pages --dotfiles -a -d docs-build",
    "docs:deploy-for-branch": "deploy-directory-on-branch-to-gh-pages -d docs-build",
    "lint": "eslint components/ pages/ utils/",
    "lint:fix": "yarn lint --fix",
    "test": "jest --config ../../node_modules/@transferwise/test-config/test/jest.lint.js"
  },
  "repository": {
    "type": "git",
    "fullname": "transferwise/neptune-web",
    "url": "git+https://github.com/transferwise/neptune-web.git"
  },
  "keywords": [],
  "author": {
    "name": "TransferWise",
    "url": "https://transferwise.com/"
  },
  "bugs": {
    "url": "https://github.com/transferwise/neptune-web/issues"
  },
  "homepage": "https://transferwise.github.io/neptune-web",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@mapbox/rehype-prism": "^0.4.0",
    "@mdx-js/loader": "^1.5.8",
    "@next/mdx": "^9.0.5",
    "@transferwise/components": "^27.8.0",
    "@transferwise/dynamic-flows": "^2.2.14",
    "@transferwise/icons": "^1.5.0",
    "@transferwise/neptune-css": "^1.2.3",
    "@transferwise/test-config": "^1.0.2",
    "@zeit/next-css": "^1.0.1",
    "bootstrap": "github:transferwise/bootstrap#semver:^5.20.0",
    "currency-flags": "^2.1.1",
    "deploy-directory-on-branch-to-gh-pages": "^0.2.5",
    "git-branch": "^2.0.1",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "lint-staged": "^9.4.2",
    "next": "^9.3.2",
    "next-fonts": "^1.0.3",
    "next-images": "^1.1.2",
    "next-transpile-modules": "^2.3.1",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.0",
    "react-docgen": "5.3.0",
    "react-docgen-external-proptypes-handler": "^2.0.0",
    "react-live": "^2.2.2",
    "rehype-slug": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@transferwise/less-config": "^1.0.3",
    "babel-plugin-inline-react-svg": "^1.1.1"
  },
  "peerDependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  }
}
