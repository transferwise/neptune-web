{
  "name": "@transferwise/neptune-docs",
  "version": "0.1.46",
  "description": "TransferWise Design System",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev-new": "node ./docgen.js && next dev",
    "build": "node ./docgen.js && next build",
    "docs-new:build": "next export -o docs-build && touch docs-build/.nojekyll",
    "docs-new:deploy": "gh-pages --dotfiles -a -d docs-build",
    "docs-new:deploy-for-branch": "deploy-directory-on-branch-to-gh-pages -d docs-build",
    "lint": "eslint components/ pages/",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "fullname": "transferwise/neptune",
    "url": "git+https://github.com/transferwise/neptune.git"
  },
  "keywords": [],
  "author": {
    "name": "TransferWise",
    "url": "https://transferwise.com/"
  },
  "bugs": {
    "url": "https://github.com/transferwise/neptune/issues"
  },
  "homepage": "https://transferwise.github.io/neptune",
  "dependencies": {
    "@mapbox/rehype-prism": "^0.3.1",
    "@mdx-js/loader": "^1.4.4",
    "@next/mdx": "^9.0.5",
    "@transferwise/components": "^18.0.0",
    "@transferwise/neptune-css": "^0.9.0",
    "@zeit/next-css": "^1.0.1",
    "bootstrap": "transferwise/bootstrap",
    "git-branch": "^2.0.1",
    "glob": "^7.1.6",
    "next": "^9.0.5",
    "next-fonts": "^0.18.0",
    "next-images": "^1.1.2",
    "next-page-map": "^1.0.1",
    "next-transpile-modules": "^2.3.1",
    "prismjs": "^1.17.1",
    "prop-types": "^15.7.2",
    "react-docgen": "5.0.0-beta.1",
    "react-docgen-external-proptypes-handler": "^2.0.0",
    "react-live": "^2.2.1",
    "refractor": "^2.10.0",
    "rehype": "^9.0.0",
    "rehype-slug": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "babel-plugin-inline-react-svg": "^1.1.0"
  },
  "peerDependencies": {
    "@transferwise/eslint-config": "^2.1.0",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": "@transferwise/eslint-config/.prettierrc.js"
}
