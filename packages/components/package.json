{
  "name": "@transferwise/components",
  "version": "10.6.0",
  "description": "TransferWise styleguide components in react",
  "license": "MIT",
  "main": "./build/main.js",
  "repository": {
    "fullname": "transferwise/components",
    "type": "git",
    "url": "git+https://github.com/transferwise/components.git"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-destructuring": "^7.4.4",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-react-jsx-self": "^7.2.0",
    "@babel/plugin-transform-react-jsx-source": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@svgr/webpack": "4.1.0",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.0",
    "bootstrap": "github:transferwise/bootstrap",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "2.1.1",
    "currency-flags": "^1.2.1",
    "deploy-directory-on-branch-to-gh-pages": "^0.2.5",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "jest": "24.7.1",
    "jest-cli": "^24.8.0",
    "jest-emotion": "^10.0.14",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-resolve": "24.7.1",
    "jest-watch-typeahead": "0.3.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.0.2",
    "lodash.times": "^4.3.2",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "prop-types": "^15.6.1",
    "react": "16.8.6",
    "react-dev-utils": "^9.0.1",
    "react-dom": "16.8.6",
    "release-to-github-with-changelog": "^1.2.2",
    "resolve": "1.10.0",
    "semver": "6.0.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.3",
    "ts-pnp": "1.1.2",
    "url-loader": "1.1.2",
    "webpack": "^4.35.3",
    "webpack-dev-server": "^3.7.2",
    "webpack-manifest-plugin": "^2.0.4",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "peerDependencies": {
    "bootstrap": "github:transferwise/bootstrap",
    "currency-flags": "^1.2.1",
    "prop-types": "^15.6.1",
    "react": "16.8.6",
    "react-dev-utils": "^9.0.1",
    "react-dom": "16.8.6"
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@transferwise/formatting": "^1.4.3",
    "@transferwise/icons": "^1.0.0",
    "@transferwise/neptune-css": "^0.5.4",
    "classnames": "^2.2.6",
    "lodash": "^4.17.14",
    "react-app-polyfill": "^1.0.1",
    "react-transition-group": "^4.2.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node ./emotionFix.js && node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom --runInBand",
    "test:watch": "jest --watch --env=jsdom",
    "pretest": "npm run check-format && npm run check-release-format",
    "check-release-format": "node_modules/.bin/release-to-github-with-changelog-pre-release-checks",
    "check-format": "prettier-eslint 'src/**/*.js' --list-different",
    "fix-format": "prettier-eslint '**/*.js' --write",
    "release-to-github": "release-to-github-with-changelog",
    "predeploy": "npm run build",
    "build-docs": "node scripts/build-docs.js",
    "deploy-docs": "gh-pages -a -d docs-build",
    "deploy-docs-for-branch": "deploy-directory-on-branch-to-gh-pages -d docs-build"
  },
  "browserslist": {
    "production": [
      ">1%",
      "last 2 versions",
      "not dead"
    ],
    "development": [
      ">1%",
      "last 2 versions",
      "not dead"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint '**/*.js' --write",
      "git add"
    ]
  }
}
