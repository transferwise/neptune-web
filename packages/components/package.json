{
  "name": "@transferwise/components",
  "version": "16.3.1",
  "description": "TransferWise styleguide components in react",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "style": "./build/main.css",
  "sideEffects": [
    "*.css"
  ],
  "repository": {
    "type": "git",
    "fullname": "transferwise/neptune",
    "url": "git+https://github.com/transferwise/neptune.git"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:webpack:deprecated dev:watch-less  ",
    "dev-new": "npm-run-all --parallel dev:babel dev:watch-less  ",
    "dev:webpack:deprecated": "node scripts/start.js",
    "dev:babel": "babel src -d lib --ignore '**/*.spec.js','**/*.docs.js' -w",
    "dev:watch-less": "gulp lessWatcher",
    "build": "npm-run-all build:*",
    "build:clean": "rm -rf lib build",
    "build:emotion-fix": "node ./emotionFix.js",
    "build:compile-css": "gulp lessCompiler",
    "build-failing-on-circle-ci-to-reintroduce:umd": "rollup -c",
    "build:es2015": "babel src -d lib --ignore '**/*.spec.js','**/*.docs.js'",
    "build:copy-files": "cpx 'src/**/!(db)/*.{css,json,svg}' lib",
    "test": "gulp lessCompiler && jest --env=jsdom --runInBand",
    "test:watch": "jest --watch --env=jsdom",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "npm run lint -- --fix",
    "docs:build": "gulp lessCompiler && node scripts/build-docs.js",
    "docs:deploy": "gh-pages -a -d docs-build",
    "docs:deploy-for-branch": "deploy-directory-on-branch-to-gh-pages -d docs-build",
    "predeploy": "npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-destructuring": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-react-jsx-self": "^7.2.0",
    "@babel/plugin-transform-react-jsx-source": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@svgr/webpack": "4.3.3",
    "@transferwise/eslint-config": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "1.6.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.0",
    "bootstrap": "github:transferwise/bootstrap#v5.18.15",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "cpx": "^1.5.0",
    "cssnano": "^4.1.10",
    "currency-flags": "^1.2.1",
    "deploy-directory-on-branch-to-gh-pages": "^0.2.5",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "3.0.2",
    "eslint-plugin-flowtype": "4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "4.2.0",
    "fs-extra": "8.1.0",
    "gulp": "^4.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-dependents": "^1.2.3",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-print": "^5.0.2",
    "html-webpack-plugin": "4.0.0-beta.8",
    "husky": "^2.7.0",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^2.1.1",
    "jest": "24.7.1",
    "jest-cli": "^24.8.0",
    "jest-emotion": "^10.0.14",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-resolve": "24.7.1",
    "jest-watch-typeahead": "0.4.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.4.2",
    "lodash.times": "^4.3.2",
    "mini-css-extract-plugin": "0.8.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.0.2",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.6.1",
    "react": "16.8.6",
    "react-dev-utils": "^9.0.1",
    "react-dom": "16.8.6",
    "react-test-renderer": "16.8.6",
    "resolve": "1.12.0",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-babel-minify": "^9.0.0",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "semver": "6.3.0",
    "style-loader": "1.0.0",
    "terser-webpack-plugin": "1.2.3",
    "ts-pnp": "1.1.4",
    "url-loader": "1.1.2",
    "webpack": "^4.35.3",
    "webpack-dev-server": "^3.7.2",
    "webpack-manifest-plugin": "^2.0.4",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "peerDependencies": {
    "bootstrap": "github:transferwise/bootstrap#v5.18.15",
    "currency-flags": "^1.2.1",
    "prop-types": "^15.6.1",
    "react": "16.8.6",
    "react-dev-utils": "^9.0.1",
    "react-dom": "16.8.6"
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@transferwise/formatting": "^2.1.0",
    "@transferwise/icons": "^1.0.0",
    "@transferwise/neptune-css": "^0.8.13",
    "classnames": "^2.2.6",
    "lodash.clamp": "^4.0.3",
    "lodash.debounce": "^4.0.8",
    "lodash.topairs": "^4.3.0",
    "react-app-polyfill": "^1.0.1",
    "react-spring": "^8.0.27",
    "react-transition-group": "^4.2.1"
  },
  "browserslist": {
    "production": [
      ">1%",
      "last 2 versions",
      "not dead"
    ],
    "development": [
      ">1%",
      "last 2 versions",
      "not dead"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|build-docs|docs|node_modules|scripts)[/\\\\]"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "../../node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": "@transferwise/eslint-config/.prettierrc.js"
}
